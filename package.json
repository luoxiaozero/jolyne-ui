    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/marked": "^3.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "highlight.js": "^11.2.0",
    "marked": "^3.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-router": "^4.0.11",
    "vue-tsc": "^0.34.17"
  },
  "author": "luoxiaozero",
  "license": "MIT",
  "files": [
    "package.json",
    "lib"
