{
  "name": "jolyne-ui",
  "version": "0.0.4-alpha.3",
  "description": "一个 Vue3 的 UI 库",
  "main": "lib/index.js",
  "scripts": {
    "dev": "vite",
    "build:lib": "rimraf lib && tsc -b ./build.tsconfig.json && gulp",
    "build:publish": "npm run build:lib && npm publish --access=public",
    "build:site": "vue-tsc --noEmit && vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.6",
    "@vicons/ionicons5": "^0.11.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/marked": "^3.0.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.6",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "highlight.js": "^11.2.0",
    "marked": "^3.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2",
    "vite": "^2.5.2",
    "vue-router": "^4.0.11",
    "vue-tsc": "^0.2.2"
  },
  "author": "luoxiaozero",
  "license": "MIT",
  "files": [
    "package.json",
    "lib"
  ],
