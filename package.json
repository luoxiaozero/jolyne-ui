    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "highlight.js": "^11.6.0",
    "marked": "^4.0.18",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.39.2"
  },
  "author": "luoxiaozero",
  "license": "MIT",
  "files": [
    "package.json",
